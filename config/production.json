{
  "log": {
    "transports": {
      "Console": {
        "level": "debug",
        "colorize": true,
        "prettyPrint": true,
        "humanReadableUnhandledException": true,
        "showLevel": true
      },
      "File": {
        "filename": "./log/revsw-stats-api.log",
        "level": "debug"
      },
      "Rsyslog": {
        "Level" : "info",
        "app-name" : "revsw-stats-api"
      }
    }
  },
  "service": {
    "https_port": 443,
    "key_path": "./config/ssl_certs/server.key",
    "cert_path": "./config/ssl_certs/server.crt",
    "elastic_es": {
      "host": "iad02-es05.revsw.net:9200",
      "requestTimeout": 60000,
      "log": [{
        "type": "stdio",
        "levels": [ "error", "warning" ]
      }]
    },
    "elastic_esurl": {
      "host": "iad02-esurl01.revsw.net:9200",
      "requestTimeout": 60000,
      "log": [{
        "type": "stdio",
        "levels": [ "error", "warning" ]
      }]
    },
    "upload_size": 1000,
    "send_tries_num": 3,
    "max_requests": 100,
    "queue_clear_timeout": 60000,
    "key_id": {
      "db_connection": "mongodb://IAD02-CMDB01.REVSW.NET:27017/revportal",
      "collection": "App",
      "poll_interval": 300000
    }
  }
}
