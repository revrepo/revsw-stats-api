{
  "log": {
    "transports": {
      "Console": {
        "level": "debug",
        "colorize": true,
        "prettyPrint": true,
        "humanReadableUnhandledException": true,
        "showLevel": true
      },
      "File": {
        "silent": true
      },
      "Rsyslog": {
        "silent": true
      }
    }
  },
  "service": {
    "queue_clear_timeout": 5000,
    "https_port": 443,
    "key_path": "./config/ssl_certs/server.key",
    "cert_path": "./config/ssl_certs/server.crt"
  }
}
